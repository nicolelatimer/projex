//create array and find average


public class Main {
  public static void main(String[] args) {

  int[] num = new int[] {94, 10, 9, 70, 2, 9};
  int sum = 0;
  
  for (int i=0; i<num.length; i++)
    sum=sum+num[i];
    
    double aver = sum/num.length;
    System.out.print("Average: " + aver);

 }
}
